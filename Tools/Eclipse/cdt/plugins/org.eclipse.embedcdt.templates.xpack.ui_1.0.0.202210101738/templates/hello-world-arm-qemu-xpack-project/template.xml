						<element value="&quot;${workspace_loc:/$(projectName)}/xpacks/micro-os-plus-architecture-aarch64/include&quot;" />
						<element value="&quot;${workspace_loc:/$(projectName)}/xpacks/micro-os-plus-devices-qemu-aarch64/include&quot;" />
						<element value="&quot;${workspace_loc:/$(projectName)}/xpacks/micro-os-plus-diag-trace/include&quot;" />
						<element value="&quot;${workspace_loc:/$(projectName)}/xpacks/micro-os-plus-semihosting/include&quot;" />
						<element value="&quot;${workspace_loc:/$(projectName)}/xpacks/micro-os-plus-startup/include&quot;" />
					</simple-array>
					<simple
						name="path"
						value="" />
					<simple
						name="buildType"
						value="" />
				</element>
			</complex-array>
		</process>
		
		<process
			type="org.eclipse.embedcdt.templates.core.ConditionalAppendToMBSStringListOptionValues">
			<simple
				name="projectName"
				value="$(projectName)" />
			<simple
				name="condition"
				value="" />
