						name="id"
						value="ilg.gnuarmeclipse.managedbuild.cross.option.*.include.paths" />
					<simple-array name="values">
						<element value="&quot;${workspace_loc:/$(projectName)}/platform-qemu-$(HWcoreArm)/include-config&quot;" />
						<element value="&quot;${workspace_loc:/$(projectName)}/platform-qemu-$(HWcoreArm)/include-platform&quot;" />
					</simple-array>
					<simple
						name="path"
						value="" />
					<simple
						name="buildType"
						value="" />
				</element>
			</complex-array>
		</process>
	</if>

	<!-- ================================================================== -->

	<!-- Add the source paths -->
	<if condition="1==1">
		<process type="org.eclipse.embedcdt.templates.core.ConditionalAddSourceFolders">
			<simple
				name="projectName"
				value="$(projectName)" />
			<simple
				name="condition"
				value="$(HWcoreArm) == cortex-m7f" />
			<simple-array name="paths">
				<element value="src" />
				<element value="xpacks/micro-os-plus-architecture-cortexm/src" />
				<element value="xpacks/micro-os-plus-devices-qemu-cortexm/src" />
				<element value="xpacks/micro-os-plus-diag-trace/src" />
				<element value="xpacks/micro-os-plus-semihosting/src" />
				<element value="xpacks/micro-os-plus-startup/src" />
			</simple-array>
		</process>

		<process type="org.eclipse.embedcdt.templates.core.ConditionalAddSourceFolders">
			<simple
				name="projectName"
				value="$(projectName)" />
			<simple
				name="condition"
				value="$(HWcoreArm) == cortex-m0" />
			<simple-array name="paths">
