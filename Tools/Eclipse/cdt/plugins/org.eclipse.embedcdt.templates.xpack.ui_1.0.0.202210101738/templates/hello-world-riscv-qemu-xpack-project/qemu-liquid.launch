    <booleanAttribute key="ilg.gnumcueclipse.debug.gdbjtag.qemu.doDebugInRam" value="true"/>
{% endif %}
    <stringAttribute key="org.eclipse.cdt.launch.PROGRAM_NAME" value="Debug/{{ projectName }}.elf"/>
