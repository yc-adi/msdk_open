			</complex-array>
		</process>

		<process
			type="org.eclipse.embedcdt.templates.core.ConditionalSetMBSBooleanOptionValue">
			<simple
				name="projectName"
				value="$(projectName)" />
			<simple
				name="condition"
				value="" />
			<complex-array name="resourcePaths">
				<element>
					<simple
						name="id"
						value="ilg.gnuarmeclipse.managedbuild.cross.option.*.linker.nostart" />
					<simple
						name="value"
						value="true" />
					<simple
						name="path"
						value="" />
					<simple
						name="buildType"
						value="" />
				</element>
			</complex-array>
		</process>
	</if>

	<if condition="$(useNanoArm)==true">
		<process
			type="org.eclipse.embedcdt.templates.core.ConditionalSetMBSBooleanOptionValue">
			<simple
				name="projectName"
				value="$(projectName)" />
			<simple
				name="condition"
				value="$(HWcoreArm) != cortex-a72" />
			<complex-array name="resourcePaths">
				<element>
					<simple
						name="id"
						value="ilg.gnuarmeclipse.managedbuild.cross.option.*.linker.usenewlibnano" />
					<simple
						name="value"
						value="true" />
					<simple
						name="path"
						value="" />
					<simple
						name="buildType"
						value="" />
				</element>
			</complex-array>
		</process>
	</if>

	<!-- ================================================================== -->

	<!-- Open main.* in editor -->
	<process type="org.eclipse.cdt.ui.OpenFiles">
		<simple
			name="projectName"
			value="$(projectName)" />
		<complex-array name="files">
			<element>
				<simple
					name="target"
					value="src/main.$(fileExtension)" />
			</element>
		</complex-array>
	</process>
</template>