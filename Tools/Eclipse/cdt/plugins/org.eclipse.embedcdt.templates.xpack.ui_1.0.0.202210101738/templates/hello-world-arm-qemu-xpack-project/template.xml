		</process>
		
	</if>

	<!-- ================================================================== -->

	<!-- Target options -->
	<if condition="1==1">
		<process type="org.eclipse.embedcdt.templates.core.ConditionalSetMBSStringOptionValue">
			<simple
				name="projectName"
				value="$(projectName)" />
			<simple
				name="condition"
				value="$(HWcoreArm) == cortex-m7f" />
			<complex-array name="resourcePaths">
				<element>
					<simple
						name="id"
						value="ilg.gnuarmeclipse.managedbuild.cross.option.arm.target.family" />
					<simple
						name="value"
						value="ilg.gnuarmeclipse.managedbuild.cross.option.arm.target.mcpu.cortex-m7" />
					<simple
						name="path"
