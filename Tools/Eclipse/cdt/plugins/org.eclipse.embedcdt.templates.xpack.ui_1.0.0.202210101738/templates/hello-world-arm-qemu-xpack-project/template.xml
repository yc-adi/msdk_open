			<simple
				name="projectName"
				value="$(projectName)" />
			<simple
				name="condition"
				value="$(HWcoreArm) == cortex-a72" />
			<simple-array name="paths">
				<element value="src" />
				<element value="xpacks/micro-os-plus-architecture-aarch64/src" />
				<element value="xpacks/micro-os-plus-devices-qemu-aarch64/src" />
