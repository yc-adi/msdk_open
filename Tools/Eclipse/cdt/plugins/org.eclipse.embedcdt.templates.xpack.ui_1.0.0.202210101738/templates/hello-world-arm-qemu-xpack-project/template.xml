 atSPDX-License-Identifier: EPL-2.0"
	id="HW.arm.qemu.xpack.exe.c.project"
	label="%HW.template.arm.label"
	description="%HW.template.arm.description"
	help="help.html">
	<property-group
		id="chip"
		label="%HW.group.target.arm.label"
		description="%HW.group.target.arm.description"
		type="PAGES-ONLY"
		help="help.html">
		<property
			id="HWcoreArm"
			label="%HWcore.arm.label"
			description="%HWcore.arm.description"
			type="select"
			default="cortex-m7f"
			hidden="false"
			persist="true">
			<item
				value="cortex-m7f"
				label="Cortex-M7F (mps2-an500)" />
			<item
				value="cortex-m0"
				label="Cortex-M0 (mps2-an385)" />
			<item
				value="cortex-a15"
				label="Cortex-A15 (virt AArch32)" />
			<item
				value="cortex-a72"
				label="Cortex-A72 (virt AArch64)" />
		</property>

		<!-- ============================================================== -->

		<property
			id="useSomeWarningsArm"
			label="%someWarnings.arm.label"
			description="%someWarnings.arm.description"
			type="boolean"
			default="true"
			hidden="false"
			persist="true" />
		<property
			id="useMostWarningsArm"
			label="%mostWarnings.arm.label"
			description="%mostWarnings.arm.description"
			type="boolean"
			default="false"
			hidden="false"
			persist="true" />
		<property
			id="useWerrorArm"
			label="%useWerror.arm.label"
			description="%useWerror.arm.description"
			type="boolean"
			default="false"
			hidden="false"
			persist="true" />
		<property
			id="doNotUseExceptionsArm"
			label="%doNotUseExceptions.arm.label"
			description="%doNotUseExceptions.arm.description"
			type="boolean"
			default="false"
			hidden="false"
			persist="true" />
		<property
			id="useOgArm"
			label="%useOg.arm.label"
			description="%useOg.arm.description"
			type="boolean"
			default="false"
			hidden="false"
			persist="true" />
		<property
			id="useNanoArm"
			label="%useNano.arm.label"
			description="%useNano.arm.description"
			type="boolean"
			default="false"
			hidden="false"
			persist="true" />
	</property-group>

	<property-group
		id="folders"
		label="%group.folders.arm.label"
		description="%group.folders.arm.description"
		type="PAGES-ONLY"
		help="help.html">

		<!-- ===== Computed =============================================== -->
		<property
			id="fileExtension"
			label="Source file extension"
			description="Will be set to cpp for ccnature"
			mandatory="true"
			hidden="true"
			default="c"
			persist="false" />
	</property-group>

	<!-- 
	Notes: 
	- the redundant 'condition="1==1"' is a workaround to control the 
	  evaluation order. Without it, processes are evaluated after all ifs 
	- the syntax for inner conditions requires spaces around the operators
	-->
		
	<if condition="1==1">
		<!-- Create the project, with elf artifact extension. -->
		<process type="org.eclipse.cdt.managedbuilder.core.NewManagedProject">
			<simple
				name="name"
				value="$(projectName)" />
			<simple
				name="artifactExtension"
				value="elf" />
			<simple
				name="isCProject"
				value="true" />
		</process>
	</if>
	
	<!-- Project nature -->
	<if condition="1==1">
		<!-- Set fileExtension="cpp" if ccnature; default was "c". -->
		<process type="org.eclipse.embedcdt.templates.core.SetPropertyIfHasNature">
			<simple
				name="projectName"
				value="$(projectName)" />
			<simple
				name="natureString"
				value="org.eclipse.cdt.core.ccnature" />
			<simple
				name="propertyName"
				value="fileExtension" />
			<simple
				name="propertyValue"
				value="cpp" />
		</process>

		<!-- Add ccnature always. -->
		<process type="org.eclipse.cdt.core.AddNature">
			<simple
				name="projectName"
				value="$(projectName)" />
			<simple
				name="natureId"
				value="org.eclipse.cdt.core.ccnature" />
		</process>
	</if>

	<!-- ================================================================== -->

	<!-- Generate project & satisfy dependencies -->
	<if condition="1==1">

		<process
			type="org.eclipse.embedcdt.ui.ConditionalRunCommand">
			<simple
				name="projectName"
				value="$(projectName)" />
			<simple
				name="condition"
				value="" />
			<simple
				name="cwd"
				value="${workspace_loc:/$(projectName)}" />
			<simple
				name="command"
				value="xpm" />
			<simple-array name="args">
				<element value="init" />
				<element value="--template" />
				<element value="@micro-os-plus/hello-world-qemu-template@1.4.1" />
				<element value="--property" />
				<element value="target=$(HWcoreArm)" />
				<element value="--property" />
				<element value="language=$(fileExtension)" />
				<element value="--property" />
				<element value="buildGenerator=cmake" />
			</simple-array>
		</process>

		<process
			type="org.eclipse.embedcdt.ui.ConditionalRunCommand">
			<simple
				name="projectName"
				value="$(projectName)" />
			<simple
				name="condition"
				value="" />
			<simple
				name="cwd"
				value="${workspace_loc:/$(projectName)}" />
			<simple
				name="command"
				value="xpm" />
			<simple-array name="args">
				<element value="install" />
			</simple-array>
		</process>
	</if>

	<!-- ================================================================== -->
	
	<if condition="1==1">
		<process type="org.eclipse.embedcdt.templates.core.ConditionalAddLiquidFiles">
			<simple
				name="projectName"
				value="$(projectName)" />
			<simple
				name="condition"
				value="" />
			<complex-array name="files">
				<element>
					<simple
						name="source"
						value="qemu-liquid.launch" />
					<simple
						name="target"
						value="$(projectName)-debug-qemu.launch" />
					<simple
						name="replaceable"
						value="true" />
				</element>
			</complex-array>
		</process>
	</if>

	<!-- ================================================================== -->
	
	<!-- Macro definitions -->
	<if condition="1==1">
		<process
			type="org.eclipse.embedcdt.templates.core.ConditionalAppendToMBSStringListOptionValues">
			<simple
				name="projectName"
				value="$(projectName)" />
			<simple
				name="condition"
				value="$(HWcoreArm) == cortex-m7f" />
			<complex-array name="resourcePaths">
				<element>
					<simple
						name="id"
						value="ilg.gnuarmeclipse.managedbuild.cross.option.*.defs" />
					<simple-array name="values">
						<element value="DEVICE_QEMU_CORTEX_M7" />
						<element value="PLATFORM_QEMU_CORTEX_M7" />
					</simple-array>
					<simple
						name="path"
						value="" />
					<simple
						name="buildType"
						value="" />
				</element>
			</complex-array>
		</process>
		<process
			type="org.eclipse.embedcdt.templates.core.ConditionalAppendToMBSStringListOptionValues">
			<simple
				name="projectName"
				value="$(projectName)" />
			<simple
				name="condition"
				value="$(HWcoreArm) == cortex-m0" />
			<complex-array name="resourcePaths">
				<element>
					<simple
						name="id"
						value="ilg.gnuarmeclipse.managedbuild.cross.option.*.defs" />
					<simple-array name="values">
						<element value="DEVICE_QEMU_CORTEX_M0" />
						<element value="PLATFORM_QEMU_CORTEX_M0" />
					</simple-array>
					<simple
						name="path"
						value="" />
					<simple
						name="buildType"
						value="" />
				</element>
			</complex-array>
		</process>
		<process
			type="org.eclipse.embedcdt.templates.core.ConditionalAppendToMBSStringListOptionValues">
			<simple
				name="projectName"
				value="$(projectName)" />
			<simple
				name="condition"
				value="$(HWcoreArm) == cortex-a15" />
			<complex-array name="resourcePaths">
				<element>
					<simple
						name="id"
						value="ilg.gnuarmeclipse.managedbuild.cross.option.*.defs" />
					<simple-array name="values">
						<element value="DEVICE_QEMU_CORTEX_A15" />
						<element value="PLATFORM_QEMU_CORTEX_A15" />
					</simple-array>
					<simple
						name="path"
						value="" />
					<simple
						name="buildType"
						value="" />
				</element>
			</complex-array>
		</process>
		<process
			type="org.eclipse.embedcdt.templates.core.ConditionalAppendToMBSStringListOptionValues">
			<simple
				name="projectName"
				value="$(projectName)" />
			<simple
				name="condition"
				value="$(HWcoreArm) == cortex-a72" />
			<complex-array name="resourcePaths">
				<element>
					<simple
						name="id"
						value="ilg.gnuarmeclipse.managedbuild.cross.option.*.defs" />
					<simple-array name="values">
						<element value="DEVICE_QEMU_CORTEX_A72" />
						<element value="PLATFORM_QEMU_CORTEX_A72" />
					</simple-array>
					<simple
						name="path"
						value="" />
					<simple
						name="buildType"
						value="" />
				</element>
			</complex-array>
		</process>
	</if>

	<if condition="1==1">
		<process
			type="org.eclipse.embedcdt.templates.core.ConditionalAppendToMBSStringListOptionValues">
			<simple
				name="projectName"
				value="$(projectName)" />
			<simple
				name="condition"
				value="" />
			<complex-array name="resourcePaths">
				<element>
					<simple
						name="id"
						value="ilg.gnuarmeclipse.managedbuild.cross.option.*.defs" />
