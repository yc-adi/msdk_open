						<element value="&quot;${workspace_loc:/$(projectName)}/xpacks/micro-os-plus-architecture-cortexm/include&quot;" />
						<element value="&quot;${workspace_loc:/$(projectName)}/xpacks/micro-os-plus-devices-qemu-cortexm/include&quot;" />
						<element value="&quot;${workspace_loc:/$(projectName)}/xpacks/micro-os-plus-diag-trace/include&quot;" />
						<element value="&quot;${workspace_loc:/$(projectName)}/xpacks/micro-os-plus-semihosting/include&quot;" />
						<element value="&quot;${workspace_loc:/$(projectName)}/xpacks/micro-os-plus-startup/include&quot;" />
						<element value="&quot;${workspace_loc:/$(projectName)}/xpacks/xpack-3rd-party-arm-cmsis-core/CMSIS/Core/Include&quot;" />
					</simple-array>
					<simple
						name="path"
						value="" />
					<simple
						name="buildType"
						value="" />
				</element>
			</complex-array>
		</process>

		<process
			type="org.eclipse.embedcdt.templates.core.ConditionalAppendToMBSStringListOptionValues">
			<simple
				name="projectName"
				value="$(projectName)" />
			<simple
				name="condition"
				value="$(HWcoreArm) == cortex-a15" />
			<complex-array name="resourcePaths">
				<element>
					<simple
						name="id"
						value="ilg.gnuarmeclipse.managedbuild.cross.option.*.include.paths" />
					<simple-array name="values">
						<element value="&quot;${workspace_loc:/$(projectName)}/include&quot;" />
						<element value="&quot;${workspace_loc:/$(projectName)}/xpacks/micro-os-plus-architecture-aarch32/include&quot;" />
						<element value="&quot;${workspace_loc:/$(projectName)}/xpacks/micro-os-plus-devices-qemu-aarch32/include&quot;" />
						<element value="&quot;${workspace_loc:/$(projectName)}/xpacks/micro-os-plus-diag-trace/include&quot;" />
						<element value="&quot;${workspace_loc:/$(projectName)}/xpacks/micro-os-plus-semihosting/include&quot;" />
						<element value="&quot;${workspace_loc:/$(projectName)}/xpacks/micro-os-plus-startup/include&quot;" />
						<element value="&quot;${workspace_loc:/$(projectName)}/xpacks/xpack-3rd-party-arm-cmsis-core/CMSIS/Core/Include&quot;" />
					</simple-array>
					<simple
						name="path"
						value="" />
					<simple
						name="buildType"
						value="" />
				</element>
			</complex-array>
		</process>

		<process
			type="org.eclipse.embedcdt.templates.core.ConditionalAppendToMBSStringListOptionValues">
			<simple
				name="projectName"
				value="$(projectName)" />
			<simple
				name="condition"
				value="$(HWcoreArm) == cortex-a72" />
			<complex-array name="resourcePaths">
				<element>
					<simple
						name="id"
						value="ilg.gnuarmeclipse.managedbuild.cross.option.*.include.paths" />
					<simple-array name="values">
